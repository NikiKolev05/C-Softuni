using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace uprajnenie_1
{
    class Program
    {
        static void Main(string[] args)
        {
            double radiant = double.Parse(Console.ReadLine());
            double gradus = radiant * 180 / Math.PI;
            Console.WriteLine(gradus);
        }
    }
}
