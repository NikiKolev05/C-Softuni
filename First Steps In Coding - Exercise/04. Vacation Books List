using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace uprajnenie_1
{
    class Program
    {
        static void Main(string[] args)
        {
            int stranici = int.Parse(Console.ReadLine());
            int stranicizachas = int.Parse(Console.ReadLine());
            int dnizakniga = int.Parse(Console.ReadLine());
            int chasove = stranici / stranicizachas;
            int result = chasove / dnizakniga;
            Console.WriteLine(result);
        }
    }
}
