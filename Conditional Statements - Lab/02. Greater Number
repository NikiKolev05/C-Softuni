using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace lekciq_2
{
    class Program
    {
        static void Main(string[] args)
        {
            int chislopurvo = int.Parse(Console.ReadLine());
            int chislovtoro = int.Parse(Console.ReadLine());
            if (chislopurvo<chislovtoro)
            {
                Console.WriteLine(chislovtoro);
            }
            else
            {
                Console.WriteLine(chislopurvo);
            }

        }
    }
}
